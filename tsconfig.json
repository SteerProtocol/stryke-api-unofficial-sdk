{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": [
    "apis/**/*",
    "models/**/*",
    "runtime.ts",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
} 