{
  "name": "@steerprotocol/stryke-api-unofficial-sdk",
  "version": "1.0.0",
  "description": "TypeScript client for the Stryke API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steerprotocol/stryke-api-unofficial-sdk.git"
  },
  "keywords": [
    "stryke",
    "api",
    "client",
    "typescript",
    "openapi",
    "swagger"
  ],
  "author": "Steer Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steerprotocol/stryke-api-unofficial-sdk/issues"
  },
  "homepage": "https://github.com/steerprotocol/stryke-api-unofficial-sdk#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "url": "^0.11.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
